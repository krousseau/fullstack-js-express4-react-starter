{
  "name": "fullstack-js-express4-react-starter",
  "version": "0.0.1",
  "description": "Starter for express 4 w/ postgres and passportJS for auth",
  "main": "server.js",
  "scripts": {
    "start": "node --harmony server.js",
    "postinstall": "gulp build"
  },
  "dependencies": {
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^2.9.14",
    "body-parser": "^1.12.2",
    "bootstrap-sass": "^3.2.0",
    "config": "^1.12.0",
    "connect-flash": "^0.1.1",
    "debug": "^2.1.3",
    "del": "^1.1.1",
    "eslint": "^0.17.1",
    "eslint-loader": "^0.7.0",
    "express": "~4.9.x",
    "express-session": "^1.10.4",
    "gulp": "^3.8.11",
    "gulp-debug": "^2.0.1",
    "gulp-eslint": "^0.7.0",
    "gulp-livereload": "^3.8.0",
    "gulp-plumber": "^1.0.0",
    "gulp-run": "^1.6.6",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-webpack": "^1.3.0",
    "gulp-webpack-build": "^0.6.0",
    "jade": "^1.9.2",
    "morgan": "^1.5.2",
    "mustbe": "^0.3.1",
    "node-config": "0.0.2",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "pg": "4.x",
    "pg-hstore": "^2.3.1",
    "react": "^0.13.1",
    "react-flux": "0.0.20",
    "react-router": "^0.13.1",
    "reflux": "^0.2.7",
    "sequelize": "^2.0.4",
    "webpack": "^1.7.3"
  },
  "engines": {
    "node": "0.12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krousseau/fullstack-js-express4-react-starter"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "postgres",
    "mustbe",
    "sequelize",
    "passportJs",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {},
  "paths": {
    "source": {
      "css": [
        "front-end/src/css/*.scss"
      ],
      "js": "front-end/src/js/**/*.*",
      "serverJs": [
        "server.js",
        "routes/**/*.js",
        "config/**/*.js",
        "models/**/*.js"
      ]
    },
    "dest": {
      "css": "./public/css",
      "js": "./public/js"
    }
  }
}
